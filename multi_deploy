pipeline { 
    agent any
    stages {
        stage('Prepare') { 
            steps { 
                script {
                	
                	SVN_APPLICATION_NAME.tokenize(",")[].eachWithIndex { appName, index ->
    					println "${index+1}. ${appName}"
					}
                }
            }
        }
        stage('Build'){
            steps {
                script {
                	print SVN_APPLICATION_NAME
                }
            }
        }

    }
}