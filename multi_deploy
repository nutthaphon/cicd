pipeline { 
    agent any
    stages {
        stage('Prepare') { 
            steps { 
                script {
                	
                	SVN_APPLICATION_NAME.tokenize(",").eachWithIndex { appName, index ->
    					print appName

					}
					
					build job: 'ETE Hot Deployment', 
    					parameters: [
    						[$class: 'com.cwctravel.hudson.plugins.extended_choice_parameter.ExtendedChoiceParameterValue', name: 'SVN_BRANCH', value: 'trunk'], 
    						[$class: 'com.cwctravel.hudson.plugins.extended_choice_parameter.ExtendedChoiceParameterValue', name: 'SVN_APPLICATION_NAME', value: 'amsgateway'], 
    						[$class: 'com.cwctravel.hudson.plugins.extended_choice_parameter.ExtendedChoiceParameterValue', name: 'SVN_DOMAIN_NAME', value: 'amsdomain'], 
    						string(name: 'DEPLOY_TARGET', value: 'Online Test JDK 1.8')
    					]
                }
            }
        }
        stage('Build'){
            steps {
                script {
                	print SVN_APPLICATION_NAME
                }
            }
        }

    }
}