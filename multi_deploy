pipeline { 
    agent any
    stages {
        stage('Prepare') { 
            steps { 
                script {
                	
                	['Cat', 'Dog', 'Elephant'].eachWithIndex { animalName, index ->
    					println "${index+1}. Animal ${animalName}"
					}
                }
            }
        }
        stage('Build'){
            steps {
                script {
                	print SVN_APPLCATION_NAME
                }
            }
        }

    }
}